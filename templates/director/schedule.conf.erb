Schedule {
  Name = "<%= @schedule_name %>"
  <%- if ! @enabled -%>
  Enabled = yes
  <%- end -%>
  <%- @runs.each do |run| -%>
  Run = <%= run %>
  <%- end -%>
  <%- @options.each do |(name, value)| -%>
  <%= name %> = <%= value %>
  <%- end -%>
  <%- @includes.each do |include| -%>
  @"<%= include %>"
  <%- end -%>
}
