Storage {
  Name = "<%= @storage_name %>"
  Address = "<%= @address %>"
  Password = "<%= @password %>"
  Device = <%= @device %>
  Media Type = <%= @media_type %>
  <%- @options.each do |(name, value)| -%>
  <%= name %> = <%= value %>
  <%- end -%>
  <%- @includes.each do |include| -%>
  @"<%= include %>"
  <%- end -%>
}
